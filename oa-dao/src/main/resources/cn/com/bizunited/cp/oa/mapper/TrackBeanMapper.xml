<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.com.bizunited.cp.oa.mapper.TrackBeanMapper" >
  <resultMap id="BaseResultMap" type="cn.com.bizunited.cp.oa.domain.base.Track" >
    <id column="TRACK_ID" property="trackId" jdbcType="BIGINT" />
    <result column="CUS_ID" property="cusId" jdbcType="BIGINT" />
    <result column="CUS_NAME" property="cusName" jdbcType="VARCHAR" />
    <result column="CUS_TYPE" property="cusType" jdbcType="INTEGER" />
    <result column="NEXT_TIME" property="nextTime" jdbcType="TIMESTAMP" />
    <result column="STEP" property="step" jdbcType="INTEGER" />
    <result column="TRACK_TYPE" property="trackType" jdbcType="INTEGER" />
    <result column="CONTENT" property="content" jdbcType="VARCHAR" />
    <result column="CREATE_TIME" property="createTime" jdbcType="TIMESTAMP" />
    <result column="CREATE_BY" property="createBy" jdbcType="VARCHAR" />
  </resultMap>
  <sql id="Base_Column_List" >
    TRACK_ID, CUS_ID, CUS_NAME, CUS_TYPE, NEXT_TIME, STEP, TRACK_TYPE, CONTENT, CREATE_TIME, 
    CREATE_BY
  </sql>
  <select id="getPage" resultMap="BaseResultMap" parameterType="cn.com.bizunited.cp.common.web.pagination.Page">
      SELECT <include refid="Base_Column_List"/>
      FROM tb_track
    <where>
      CUS_ID=#{advancedQueryCondition.cusId}
    </where>
    ORDER BY CREATE_TIME DESC
  </select>
</mapper>